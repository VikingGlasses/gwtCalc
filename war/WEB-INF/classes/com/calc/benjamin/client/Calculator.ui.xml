<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
		<g:VerticalPanel styleName="calculator">
			<g:FlexTable ui:field="history" styleName="display"></g:FlexTable>
			<g:HorizontalPanel ui:field="display" styleName="display">
				<g:TextBox ui:field="calcBox" styleName="calcBox"></g:TextBox>
				<g:Label ui:field="resultLabel" styleName="resultLabel"></g:Label>
			</g:HorizontalPanel>
			<g:FlowPanel ui:field="keyPad" styleName="keyPad"></g:FlowPanel>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder>